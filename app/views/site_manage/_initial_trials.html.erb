

<table class="table">
	<thead>

	
			<th style="width: 200px"> 申请时间 </th>
			<th style="width: 200px"> 用户 </th>


			<th>
			
			</th>
	
	</thead>
	<tbody>
		<% @initial_trials.each do | trial | %>
			<tr>
		
				<td>
					<%= trial.created_at.to_s.gsub!('UTC', '') %>
				</td>
				<td>
					<%= trial.user.email %>
				</td>

				
				<td align="center">
					<%= link_to 'Process', initial_trial_detail_path(trial), class: "process_button btn btn-secondary"  %>
				</td>
			</tr>
		<% end %>
	</tbody>
</table>
