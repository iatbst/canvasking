

<table class="table">
	<thead>
			<th style="width: 200px"> IP </th>
			<th style="width: 200px"> 用户 </th>
			<th style="width: 200px"> 开始时间 </th>
			<th style="width: 200px"> 过期时间 </th>
			

	
	</thead>
	<tbody>
		<% @active_servers.each do | server | %>
			<tr>
				<td>
					<%= server.ip %>
				</td>
				<td>
					<%= server.user.email %>
				</td>
				<td>
					<%= server.created_at.to_s.gsub!('UTC', '') %>
				</td>				
				<td>
					<%= server.expire_date.to_s.gsub!('UTC', '') %>
				</td>		


				
			</tr>
		<% end %>
	</tbody>
</table>
